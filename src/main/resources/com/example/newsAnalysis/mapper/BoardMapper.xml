<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.newsAnalysis.mapper.BoardMapper">
    <select id="boardCount" resultType="int">
        select count(*) as cnt
        from ranknews;
    </select>

    <select id="getList" resultType="com.example.newsAnalysis.model.RankDayVO">
        select *
        from ranknews;
    </select>
</mapper>
